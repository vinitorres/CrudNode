generator client {
  provider = "prisma-client-js"
  previewFeatures = ["driverAdapters"]
  binaryTargets = ["native", "rhel-openssl-3.0.x"]
}

datasource db {
  provider = "postgresql"
}

model Contact {
  id        Int      @id @default(autoincrement())
  name      String
  surname   String
  phone     String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
